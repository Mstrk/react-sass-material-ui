{
  "name": "react-sass-material-ui",
  "version": "0.0.0-development",
  "description": "A set of components for React, inspired on material design guide lines and preference for stylesheets.",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "dev": "webpack-dev-server --progress",
    "test": "jest --coverage",
    "prebuild": "rimraf dist",
    "build-local": "rm -rf dist && npm run build",
    "build": "NODE_ENV=production babel src --copy-files --out-dir dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mstrk/react-sass-material-ui.git"
  },
  "keywords": [
    "react",
    "react-component",
    "sass",
    "material design",
    "material-ui"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Bruno Morgado <mstrk512@gmail.com> (http://www.meetbruno.eu/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mstrk/react-sass-material-ui/issues"
  },
  "homepage": "https://github.com/mstrk/react-sass-material-ui#readme",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "classnames": "2.2.5"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.2.3",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "cross-env": "3.1.3",
    "css-hot-loader": "1.1.1",
    "css-loader": "0.28.4",
    "cz-conventional-changelog": "1.2.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "ghooks": "1.3.2",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.4",
    "node-sass": "4.5.3",
    "node-sass-json-importer": "3.0.2",
    "postcss-loader": "2.0.5",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.5.4",
    "react-hot-loader": "next",
    "react-router": "4.1.1",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.5",
    "semantic-release": "6.3.2",
    "sinon": "1.17.6",
    "style-loader": "0.18.2",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "es2015": {
            "loose": true,
            "modules": false
          }
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "prefer-const": 2,
      "no-var": 2,
      "prefer-arrow-callback": 2,
      "arrow-body-style": 2,
      "react/no-unused-prop-types": 0,
      "import/no-unresolved": 2
    },
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
